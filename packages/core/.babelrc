{
    "plugins": [
        "transform-object-rest-spread",
    ],
    "env": {
        "DEMO_PROD": {
            "presets": [[ "es2015", {modules:false} ]],
            "plugins":[
                ["module-resolver", {
                    // paths are relative do "<root>/packages/demo"
                    "root":["../core/src"],
                    "alias":{
                        "lib":"../core/lib"
                    }
                }]
            ]
        },
        "DEMO_DEV": {
            "plugins":[
                ["module-resolver", {
                    // paths are relative do "<root>/packages/demo"
                    "root":["../core/src"],
                    "alias":{
                        "lib":"../core/lib"
                    }
                }]
            ]
        }
    }
}
