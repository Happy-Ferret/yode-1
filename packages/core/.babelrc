{
    "plugins": [
        "transform-object-rest-spread",
    ],
    "env": {
        "ROLLUP": {
            "presets": [
                ["env", {
                    "modules": false
                }]
            ],
            "plugins": [
                "external-helpers",
                ["module-resolver", {
                    "root":["./src"],
                    "alias":{
                        "lib":"./lib"
                    }
                }]
            ]
        }
    }
}
